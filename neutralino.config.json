{
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
  "applicationId": "ml.video.classifier",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/dist/",
  "url": "/index.html",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "filesystem.*",
    "server.*",
    "storage.*",
    "window.*",
    "debug.log"
  ],
  "globalVariables": {

  },
  "modes": {
    "window": {
      "title": "ml-video-classifier",
      "width": 1400,
      "height": 800,
      "minWidth": 1024,
      "minHeight": 600,
      "center": true,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/src/assets/appIcon.png",
      "enableInspector": true,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": false,
      "injectGlobals": true
    }
  },
  "cli": {
    "binaryName": "ml-video-classifier",
    "resourcesPath": "/dist/",
    "binaryVersion": "6.4.0",
    "clientVersion": "6.4.0",
    "distributionPath": "/release/",
    "hostProject": {
      "projectPath": "./",
      "buildCommand": "npm run vite-build"
    }
  }
}